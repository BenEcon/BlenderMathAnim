shader complex_function(
    vector vec=(0.,0.,0.),
    output vector outVec=(0.,0.,0.))
{
    vector vx=vector(1.,0.,0.);
    vector vy=vector(0.,1.,0.);
    vector vz=vector(0.,0.,1.);
    
    float x = vec[0];
    float y = vec[1];
    float z = 0;
    
    float X = x;
    float Y = y;
    
    outVec = vector(X,Y,0);
}